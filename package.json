  "license": "MIT",
  "author": "Jason Garber <jason@sixtwothree.org> (https://sixtwothree.org)",
  "repository": "github:jgarber623/refresh-dc.org",
  "scripts": {
    "build": "eleventy --quiet",
    "clean": "rm -rf public",
    "lint": "FORCE_COLOR=1 npm-run-all --continue-on-error --serial lint:**",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint ./src/**/*.css",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "pretest": "npm run build",
    "start": "eleventy --quiet --serve",
    "test": "test -f ./public/index.html"
  },
  "devDependencies": {
    "@11ty/eleventy": "*",
    "@jgarber/eslint-config": "*",
    "cssnano": "*",
    "dayjs": "*",
    "js-yaml": "*",
    "markdown-it": "*",
    "npm-run-all": "*",
    "postcss": "*",
    "postcss-easy-import": "*",
    "postcss-nesting": "*",
    "stylelint": "*",
    "stylelint-config-standard": "*",
    "stylelint-order": "*"
  }
}
