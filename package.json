  "license": "MIT",
  "author": "Jason Garber <jason@sixtwothree.org> (https://sixtwothree.org)",
  "repository": "github:jgarber623/refresh-dc.org",
  "scripts": {
    "build": "eleventy --quiet",
    "clean": "rm -rf public",
    "lint": "FORCE_COLOR=1 npm-run-all --continue-on-error --serial lint:**",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint ./src/**/*.css",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "start": "eleventy --quiet --serve",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@11ty/eleventy": "latest",
    "cssnano": "latest",
    "dayjs": "latest",
    "eslint": "latest",
    "js-yaml": "latest",
    "markdown-it": "latest",
    "npm-run-all": "latest",
    "postcss": "latest",
    "postcss-easy-import": "latest",
    "postcss-nesting": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest",
    "stylelint-order": "latest"
  }
}
